    "url": "https://github.com/DatNT1998/clone-proshop-bradtravesy/issues"
  },
  "homepage": "https://github.com/DatNT1998/clone-proshop-bradtravesy#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.11.10"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
